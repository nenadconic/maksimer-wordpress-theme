{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": [
    "plugin:@wordpress/eslint-plugin/recommended",
    "plugin:compat/recommended"
  ],
  "rules": {
    "no-unused-vars" : "warn",
    "import/prefer-default-export" : "off"
  }
}
