{
  "name": "maksimer-wordpress-theme",
  "author": "Maksimer",
  "description": "WordPress starter theme developed and maintained by Maksimer",
  "version": "3.2.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/maksimer/maksimer-wordpress-theme.git"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@wordpress/eslint-plugin": "^3.3.0",
    "@wordpress/scripts": "^6.1.0",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "browserslist": "^4.8.2",
    "core-js": "^3.6.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-compat": "^3.3.0",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^6.2.0",
    "gulp-newer": "^1.4.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "imagemin-webpack": "^5.1.1",
    "path": "^0.12.7",
    "postcss-discard-comments": "^4.0.2",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-prettify": "^0.3.4",
    "pre-commit": "^1.2.2",
    "pump": "^3.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-stream": "^5.2.1"
  },
  "scripts": {
    "start": "npm install && gulp",
    "watch": "gulp watch",
    "build": "gulp",
    "build-release": "npm install && composer install --no-dev -o && gulp",
    "lint-release": "npm install && composer install && npm run lint",
    "lint-js": "eslint assets/js",
    "lint:css:src": "wp-scripts lint-style 'assets/**/*.scss'",
    "lint-php": "composer run lint",
    "format-js": "eslint --fix assets/js",
    "lint": "npm run lint-js && npm run lint-php",
    "format": "npm run format-js"
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "last 1 Android versions",
    "last 1 ChromeAndroid versions",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Opera versions",
    "not op_mini all"
  ],
  "dependencies": {}
}
